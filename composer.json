   "type":"library",
   "license":"MIT",
   "support":{
      "issues":"https://github.com/phpdune/framework/issues",
      "source":"https://github.com/dune/framework"
   },
   "authors":[
      {
         "name":"Abhishek B",
         "email":"phpdune@gmail.com"
      }
   ],
   "require":{
